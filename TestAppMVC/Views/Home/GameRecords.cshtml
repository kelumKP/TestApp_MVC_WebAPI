        <th>
            @Html.DisplayNameFor(model => model.PlayerA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlayerB)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Result)
        </th>
        @*<th></th>*@
    </tr>

@foreach (var item in Model) {
