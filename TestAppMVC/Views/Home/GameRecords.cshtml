@model IEnumerable<TestAppMVC.Models.GameDetailViewModel>

@{
    ViewBag.Title = "GameRecords";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Game Records</h2>

<table class="table">
